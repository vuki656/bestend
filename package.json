        "@types/prettier": "^2.7.2",
        "cspell": "^6.26.3",
        "eslint": "^8.34.0",
        "prettier": "^2.8.4",
        "typescript": "^4.9.5"
    },
    "packageManager": "yarn@3.4.1"
}
